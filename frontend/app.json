{
  "expo": {
    "name": "StreamPay",
    "slug": "streampay3",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "streampay",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#0f172a",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-image.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f172a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.streampay.app",
      "infoPlist": {
        "NSCameraUsageDescription": "StreamPay necesita acceso a la c√°mara para subir fotos de productos en el marketplace",
        "NSPhotoLibraryUsageDescription": "Accede a tus fotos para compartir en el marketplace",
        "UIViewControllerBasedStatusBarAppearance": true
      }
    },
    "android": {
      "package": "com.streampay.app",
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0f172a"
      },
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "usesCleartextTraffic": true,
      "edgeToEdgeEnabled": true,
      "allowBackup": true,
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-image.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0f172a"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "ff99e107-f144-4222-b3fa-d902c4f9f59f"
      }
    }
  }
}